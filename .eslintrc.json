  {
    "root": true,
    "env": {
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-strongly-recommended",
      "plugin:prettier/recommended"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "@typescript-eslint/parser",
      "sourceType": "module"
    },
    "rules": {
      "vue/multi-word-component-names": 0,
      "vue/attributes-order": 2
    }
  }